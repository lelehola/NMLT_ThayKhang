#include <iostream>

using namespace std;
void HoanVi(int &, int &);
void sort(int [], int );
int NguoiNhan(int [], int , int);
int main()
{
    int n;
	cin >> n;
	int SoGiai;
    cin >> SoGiai;
    int a[100];
	for(int i =0; i < n; i++)
	  cin >> a[i]
	  ;
    sort(a,n);
    int kq = NguoiNhan(a,n,SoGiai);
    cout << kq;
    return 0;

}
int NguoiNhan(int a[], int n, int SoGiai)
{   int dem1 = 0;
	for(int i = 0; i < n; i++)
	  if(a[i] == 0)
	     dem1++;
	if(dem1 == n)
	  return 0;
	int dem = 0;
	int i = 0;
    while(dem < SoGiai)
	{   
		dem++;
		int dem1 = 0;
		for(int j = i + 1; j < n; j++)
		  if(a[i] == a[j])
		    dem1++;
		dem += dem1;
		i++;
	}
	return dem;
}
void HoanVi(int &a, int &b)
{
    int temp = a;
    a = b;
    b = temp;
}
void sort(int a[], int n)
{
    for(int i = 0 ; i  < n-1; i++)
      for(int j = i+1; j < n; j++)
        if(a[i] < a[j])
          HoanVi(a[i], a[j]);
}
